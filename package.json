{
  "name": "openapi-combination-proxy-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "types": "npx openapi-fetch-typescript ./public/api/openapi.json ./app/openapi-types.ts OpenApiTypes",
    "watch-types": "nodemon --watch public/openapi.json --exec \"npm run types\"",
    "next-dev": "next dev",
    "dev": "concurrently \"next dev\" \"npm:watch-types\""
  },
  "dependencies": {
    "actionschema": "^0.0.34",
    "ajv": "^8.12.0",
    "from-anywhere": "^0.0.21",
    "next": "14.2.1",
    "qs": "^6.12.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/node": "^20",
    "@types/qs": "^6.9.15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "bun": "^1.1.4",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.1",
    "nodemon": "^3.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
